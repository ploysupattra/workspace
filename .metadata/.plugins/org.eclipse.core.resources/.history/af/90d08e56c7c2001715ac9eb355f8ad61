import javax.swing.JOptionPane;

public class changsud {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String ipusd = JOptionPane.showInputDialog("Input Price Bath");
		System.out.println(ipusd);
		float usd =(float)32.73;
		float chang = (float)(usd*Float.parseFloat(ipusd));
		JOptionPane.showMessageDialog(null, chang, "Total USD Price", JOptionPane.INFORMATION_MESSAGE);
	}

}
