import javax.swing.JOptionPane;

public class Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String fareheil = JOptionPane.showInputDialog("Plese Input Fareheil");
		System.out.println(fareheil);		
		int test = Integer.parseInt(fareheil);
		int centigrade = (5/9)*( test-32);
		JOptionPane.showMessageDialog(null, centigrade , "Total", JOptionPane.INFORMATION_MESSAGE);

	}

}
